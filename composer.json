{
  "name": "famoser/xkcd-cache",
  "type": "application",
  "license": "MIT",
  "description": "This is a cache of the comics at XKCD.",
  "keywords": [
    "xkcd",
    "cache",
    "api"
  ],
  "scripts": {
    "serve": "symfony serve --allow-http"
  },
  "authors": [
    {
      "name": "Florian Moser",
      "email": "git@famoser.ch",
      "homepage": "http://famoser.ch",
      "role": "Developer"
    }
  ],
  "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/famoser/Slim"
        }
   ],
  "require": {
    "slim/slim": "dev-famoser-mock-post",
    "slim/twig-view": "^2.1",
    "famoser/jsonmapper": "0.12.1",
    "codeguy/upload": "^1.3.2",
    "phpunit/phpunit": "^5.6",
    "swiftmailer/swiftmailer": "^6.0.1",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "^0.4",
    "squizlabs/php_codesniffer": "2.*",
    "deployer/recipes": "^6",
    "deployer/deployer": "^4",
    "codacy/coverage": "^1"
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "Famoser\\XKCDCache\\": "src",
      "Famoser\\XKCDCache\\Tests\\": "tests"
    }
  }
}
